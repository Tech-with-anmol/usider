{
  "expo": {
    "name": "Usider",
    "slug": "Usider",
    "version": "0.0.2",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.ak.usider"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-sqlite",
      "react-native-background-fetch"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "c25aaaa0-7f3a-4f28-ae95-06d83f01b271"
      }
    },
    "developmentClient": {
      "silentLaunch": false
    },
    "android": {
      "package": "com.ak.usider",
      "softwareKeyboardLayoutMode": "pan"  
    }
  }
}
